modules = ["nodejs-20", "python-3.12", "web", "bash"]
[agent]
expertMode = true
integrations = ["javascript_websocket:1.0.0"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Family Guardian 360°"

[[workflows.workflow]]
name = "Family Guardian 360°"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd src && python -m gunicorn --bind 0.0.0.0:5000 --workers 1 --worker-class geventwebsocket.gunicorn.workers.GeventWebSocketWorker --timeout 120 app:app"
waitForPort = 5000

